<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER_LEFT" prefHeight="280.0" prefWidth="320.0" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.pixeleon.khpi.oop.labfour.prfactapp.PrimeFactorsController">
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
      <HBox alignment="CENTER_LEFT" spacing="10.0">
         <Label text="From:" />
         <TextField fx:id="textFieldFrom" prefWidth="115.0" />
         <Label text="To:" />
         <TextField fx:id="textFieldTo" prefWidth="115.0" />
      </HBox>
   <TextArea fx:id="textAreaFactors" editable="false" maxWidth="-Infinity" prefWidth="310.0" wrapText="true">
      <padding>
         <Insets left="5.0" right="5.0" />
      </padding>
   </TextArea>
   <ProgressBar fx:id="progressBar" prefHeight="18.0" prefWidth="310.0" progress="0.0" />
   <HBox alignment="CENTER" spacing="10.0">
      <children>
         <Button fx:id="buttonStart" mnemonicParsing="false" onAction="#doStart" text="START" />
         <Button fx:id="buttonSuspend" mnemonicParsing="false" onAction="#doSuspend" text="SUSPEND" />
         <Button fx:id="buttonResume" mnemonicParsing="false" onAction="#doResume" text="RESUME" />
         <Button fx:id="buttonFinish" mnemonicParsing="false" onAction="#doFinish" text="FINISH" />
      </children>
   </HBox>
</VBox>
