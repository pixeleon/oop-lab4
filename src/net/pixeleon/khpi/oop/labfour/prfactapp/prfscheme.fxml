<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<FlowPane alignment="CENTER_LEFT" hgap="10.0" prefWidth="275.0" vgap="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.pixeleon.khpi.oop.labfour.prfactapp.PrimeFactorsController">
   <children>
      <Label text="From:" />
      <TextField fx:id="textFieldFrom" prefWidth="90.0" />
      <Label text="To:" />
      <TextField fx:id="textFieldTo" prefWidth="90.0" />
      <TextArea fx:id="textAreaFactors" prefHeight="260.0" prefWidth="260.0" wrapText="true" />
      <ProgressBar fx:id="progressBar" prefHeight="18.0" prefWidth="260.0" progress="0.0" />
      <Button fx:id="buttonStart" mnemonicParsing="false" onAction="#doStart" text="START" />
      <Button fx:id="buttonSuspend" mnemonicParsing="false" onAction="#doSuspend" text="SUSPEND" />
      <Button fx:id="buttonResume" mnemonicParsing="false" onAction="#doResume" text="RESUME" />
      <Button fx:id="buttonFinish" mnemonicParsing="false" onAction="#doFinish" text="FINISH" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</FlowPane>
